{
  "name": "fp-ts-iterators",
  "version": "0.0.1",
  "description": "Functional Iterable and AsyncIterable",
  "main": "build/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "compile": "tsc --project tsconfig.json --noEmit",
    "compile:watch": "npm run compile -- --watch",
    "lint": "eslint .",
    "release": "npm run test && npm run lint && npm run build && release-it",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "NODE_ENV=test jest --verbose --coverage",
    "test:watch": "NODE_ENV=test jest --verbose --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/velocityzen/fp-ts-iterators.git"
  },
  "keywords": [
    "fp-ts",
    "fp",
    "iterator",
    "async",
    "functional",
    "task",
    "parallelism",
    "parallel"
  ],
  "author": "Alexey Novikov <v@2dubs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/velocityzen/fp-ts-iterators/issues"
  },
  "homepage": "https://github.com/velocityzen/fp-ts-iterators#readme",
  "peerDependencies": {
    "fp-ts": "^2.16.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@unsplash/ts-namespace-import-plugin": "^1.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-fp-ts-matchers": "^0.4.1",
    "prettier": "^3.0.0",
    "release-it": "^17.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^4.0.0",
    "rollup-plugin-node-externals": "^7.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
